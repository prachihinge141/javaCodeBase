<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 xmlns:context="http://www.springframework.org/schema/context"
 xsi:schemaLocation="http://www.springframework.org/schema/beans
 http://www.springframework.org/schema/beans/spring-beans.xsd
 http://www.springframework.org/schema/context
 http://www.springframework.org/schema/context/spring-context.xsd">
 
 
 <context:annotation-config></context:annotation-config>
 
 <bean class="com.springcore.auto.wire.Address" name="address1" >
 <property name="streetName" value="Dehu Road"/>
 <property name="cityName" value="Pune"/>
 </bean>
 
 <bean class="com.springcore.auto.wire.Address" name="address2" >
 <property name="streetName" value="Moshi Road"/>
 <property name="cityName" value="Pune"/>
 </bean>
 
 <bean class="com.springcore.auto.wire.Emp"  name="emp" ></bean>
 
</beans>